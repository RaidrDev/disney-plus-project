c/components/ImgSlider.js",["84","85","86","87"],"import React from 'react'\nimport styled from 'styled-components';\nimport Slider from 'react-slick'\nimport \"slick-carousel/slick/slick.css\";\nimport \"slick-carousel/slick/slick-theme.css\";\n\n\nfunction ImgSlider() {\n    \n    let settings = {\n        dots: true,\n        infinite: true,\n        speed: 500,\n        slidesToShow: 1,\n        slidesToScroll: 1,\n        autoplay: true\n    }\n\n    return (\n        <Carousel {...settings}>\n            <Wrap>\n                <img src= \"/images/slider-badging.jpg\" />\n            </Wrap>\n\n            <Wrap>\n                <img src= \"/images/slider-badag.jpg\" />\n            </Wrap>\n\n            <Wrap>\n                <img src= \"/images/slider-scale.jpg\" />\n            </Wrap>\n\n            <Wrap>\n                <img src= \"/images/slider-scales.jpg\" />\n            </Wrap>\n\n        </Carousel>\n    )\n}\n\nexport default ImgSlider\n\nconst Carousel = styled(Slider)`\n    margin-top: 20px;\n\n    ul li button {\n        &:before {\n            font-size: 10px;\n            color: rgb(150, 158, 171);\n        }\n    }\n\n    li.slick-active button:before{\n        color: white;\n    }\n\n    .slick-list{\n        overflow: visible;\n    }\n\n    button{\n        z-index: 1;\n    }\n`\n\nconst Wrap = styled.div`\n    cursor: pointer;\n\n    img {\n        border: 4px solid transparent;\n        width: 100%;\n        height: 100%;\n        border-radius: 4px;\n        box-shadow: rgb(0 0 0 / 69%) 0px 26px 30px -10px,\n        rgb(0 0 0 / 73%) 0px 16px 10px -10px;\n        transition-duration: 300ms;\n        \n        &:hover{\n            border: 4px solid rgba(249, 249, 249, 0.8);\n        }\n    }\n`","/Users/jose/Documents/github/web-projects/disney-plus-clone/src/components/Movies.js",["88","89","90","91","92"],"/Users/jose/Documents/github/web-projects/disney-plus-clone/src/components/Viewers.js",["93","94","95","96","97"],"import React